{
  "name": "express-intercept",
  "description": "Build Express middleware to intercept / replace / inspect / transform response",
  "version": "0.8.0",
  "author": "Yusuke Kawasaki <u-suke@kawa.net>",
  "contributors": [
    "Yusuke Kawasaki <u-suke@kawa.net>"
  ],
  "dependencies": {
    "async-request-handler": "^0.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "express": "^4.17.1",
    "mocha": "^7.1.2",
    "supertest": "^4.0.2",
    "typescript": "^3.9.2"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "types/express-intercept.d.ts"
  ],
  "homepage": "https://github.com/kawanet/express-intercept#readme",
  "keywords": [
    "expressjs",
    "inspector",
    "interceptor",
    "middleware",
    "modify",
    "response",
    "tamper"
  ],
  "license": "MIT",
  "main": "lib/express-intercept.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/express-intercept.git"
  },
  "scripts": {
    "build": "npm run build-lib && npm run build-types",
    "build-lib": "./node_modules/.bin/tsc -p .",
    "build-types": "./node_modules/.bin/tsc -p types",
    "fetch-mwsupertest": "curl -s https://raw.githubusercontent.com/kawanet/middleware-supertest/master/lib/middleware-supertest.ts | sed 's#express-intercept#../../lib/express-intercept#' > test/lib/middleware-supertest.ts && npm run build-lib",
    "fixpack": "fixpack",
    "mocha": "./node_modules/.bin/mocha -R spec test/*.js",
    "test": "npm run mocha",
    "tsc-watch": "tsc --watch"
  },
  "typings": "./types/express-intercept.d.ts"
}
